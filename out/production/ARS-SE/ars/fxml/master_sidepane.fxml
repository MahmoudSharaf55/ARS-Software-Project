<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="772.0" prefWidth="292.0" style="-fx-background-color: #607D8B;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <BorderPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-image: url('ars/Resources/sidepane_background.jpg');">
            <left>
                <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
                    <image>
                        <Image url="@../Resources/airplane%201.png" />
                    </image>
                    <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </BorderPane.margin>
                </ImageView>
            </left>
            <bottom>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="32.0" prefWidth="199.0" text="Egypt Air" textAlignment="CENTER" textFill="#cfd8dc" BorderPane.alignment="CENTER">
                    <font>
                        <Font name="Century Gothic Bold" size="21.0" />
                    </font>
                    <BorderPane.margin>
                        <Insets bottom="10.0" />
                    </BorderPane.margin>
                </Label>
            </bottom>
        </BorderPane>
        <JFXButton accessibleText="dashboard" prefHeight="84.0" prefWidth="294.0" text="Dashboard" textFill="WHITE" />
        <JFXButton accessibleText="search" prefHeight="84.0" prefWidth="294.0" text="Search Engine" textFill="WHITE" />
        <JFXButton accessibleText="ADDFLIGHTS" prefHeight="84.0" prefWidth="294.0" text="Add Flights" textFill="WHITE" />
        <JFXButton accessibleText="Manage_Flights" prefHeight="84.0" prefWidth="294.0" text="Manage Flights" textFill="WHITE" />
        <JFXButton accessibleText="PASSENGERS" prefHeight="84.0" prefWidth="294.0" text="Passengers" textFill="WHITE" />
        <HBox accessibleText="hbox" alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
            <children>
                <JFXButton prefHeight="100.0" prefWidth="150.0" text="  Logout" textFill="WHITE">
                    <graphic>
                        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../Resources/logout_small_icon.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </JFXButton>
                <JFXButton accessibleText="exit" prefHeight="100.0" prefWidth="150.0" text="  Exit" textFill="WHITE">
                    <graphic>
                        <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../Resources/exit_small_icon.png" />
                            </image>
                            <viewport>
                                <Rectangle2D />
                            </viewport>
                        </ImageView>
                    </graphic>
                </JFXButton>
            </children>
        </HBox>
    </children>
